{
  "name": "web",
  "version": "0.5.5",
  "private": true,
  "scripts": {
    "start": "vite --clearScreen false",
    "start:docker": "yarn build:docker && docker run -p 3000:80 anime-skip-web:dev",
    "compile": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.vue --max-warnings 0 --fix",
    "lint:no-fix": "eslint . --ext .js,.ts,.vue --max-warnings 0",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "echo 'TODO'",
    "test:e2e": "echo 'TODO'",
    "build": "yarn compile && vite build",
    "build:staged": "yarn build --mode staged",
    "build:docker": "npx dotenv-cli -e docker/.env -- bash -c 'docker build --build-arg GITHUB_PACKAGES_TOKEN=${GITHUB_PACKAGES_TOKEN} . -t anime-skip-web:dev'",
    "preview": "echo 'Previewing \"dist/\" build' && vite preview"
  },
  "resolutions": {
    "vue": "3.0.11"
  },
  "dependencies": {
    "@anime-skip/axios-api": "^1.1.2",
    "@anime-skip/ui": "^0.1.4",
    "@types/node": "^14.14.37",
    "core-js": "^3.6.5",
    "vue": "3.0.11",
    "vue-recaptcha-v3": "^2.0.0",
    "vue-router": "^4.0.0-beta.4",
    "vuex": "^4.0.0-beta.4",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@anime-skip/types": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.23.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "push-dir": "^0.4.1",
    "sass": "^1.26.5",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.3",
    "vite": "^2.1.5",
    "vite-tsconfig-paths": "^2.5.0",
    "vue-tsc": "^0.0.17"
  }
}
